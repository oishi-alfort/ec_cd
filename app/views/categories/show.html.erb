<h1>Categories#show</h1>
<% if @category.cds.present? %>
	<% @category.cds.each do |cd| %>
		<div class="eachCd">
			<%= link_to cd_path(cd.id) do %>
			<%= attachment_image_tag cd, :cd_image, :fill, 100, 100, fallback: "no_image.jpg" %>
			<% end %>
			<p><%= cd.cd_title %></p>
		</div>
	<% end %>
<% end %>
